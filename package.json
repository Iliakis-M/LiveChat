    "chalk": "^2.4.1",
    "express": "^4.16.4",
    "fs-extra": "^7.0.1",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "strip-ansi": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-preset-minify": "^0.5.0"
  },
  "optionalDependencies": {
    "showdown": "^1.9.0"
